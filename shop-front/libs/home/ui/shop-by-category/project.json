{
  "name": "home-ui-shop-by-category",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/home/ui/shop-by-category/src",
  "prefix": "shop",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/home/ui/shop-by-category/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/home/ui/shop-by-category/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/home/ui/shop-by-category/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/home/ui/shop-by-category/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/home/ui/shop-by-category/**/*.ts",
          "libs/home/ui/shop-by-category/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:web", "type:ui"]
}
